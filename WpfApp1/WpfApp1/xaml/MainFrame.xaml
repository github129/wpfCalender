<Window x:Class="WpfApp1.MainFrame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainFrame" Height="750" Width="600">
    <Window.Resources>
        <DataTemplate DataType="{x:Type local:SomeCalenderWindowViewModel}">
            <local:SomeCalenderWindow/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:CalenderWindowViewModel}">
            <local:CalenderWindow/>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{Binding CurrentPage}"/>
        <ContentControl DockPanel.Dock="Bottom" >
            <Button Content="Button" HorizontalAlignment="Left" Margin="125,47,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        </ContentControl>
    </Grid>
</Window>
